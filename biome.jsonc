{
    "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
    "formatter": {
        "enabled": true
    },
    "javascript": {
        "formatter": {
            "quoteStyle": "single"
        }
    },
    "linter": {
        "enabled": false,
        "rules": {
            "style": {
                "useEnumInitializers": "off"
            }
        }
    },
    "organizeImports": {
        "enabled": true
    }
}